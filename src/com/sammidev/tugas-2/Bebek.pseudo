procedure countingSortBebek(h,N)
    for i <- 1, N do
        h[i] <- h[i]+ 1
    end for

    index <- 1
    for i <- 1, N do
        for j <- 1, h[i] do
            a[index] <- i
            index <- index + 1
        end for
    end for
end procedure